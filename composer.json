{
	"name": "habanatech/taxidrivers-corp",
	"description": "An HabanaTech project",
    "type": "project",
    "license": "proprietary",
    "require": {
		"php": "^7.1.3",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"ext-json": "*",
		"a2lix/auto-form-bundle": "0.2",
		"a2lix/translation-form-bundle": "^3.0",
		"dompdf/dompdf": "^0.8",
		"easycorp/easyadmin-bundle": "*",
		"erusev/parsedown": "dev-master",
		"friendsofsymfony/ckeditor-bundle": "*",
		"google/recaptcha": "^1.2",
		"knplabs/doctrine-behaviors": "^1.5",
		"liip/imagine-bundle": "*",
		"msgphp/user-bundle": "*",
		"oneup/flysystem-bundle": "*",
		"sensio/framework-extra-bundle": "^5.4",
		"symfony/asset": "*",
		"symfony/console": "*",
		"symfony/dotenv": "*",
		"symfony/expression-language": "*",
		"symfony/flex": "*",
		"symfony/form": "*",
		"symfony/framework-bundle": "*",
		"symfony/google-mailer": "*",
		"symfony/http-client": "*",
		"symfony/intl": "*",
		"symfony/mailer": "*",
		"symfony/messenger": "*",
		"symfony/mime": "*",
		"symfony/monolog-bundle": "*",
		"symfony/orm-pack": "*",
		"symfony/process": "*",
		"symfony/routing": "*",
		"symfony/security-bundle": "*",
		"symfony/serializer-pack": "*",
		"symfony/swiftmailer-bundle": "*",
		"symfony/translation": "*",
		"symfony/twig-pack": "*",
		"symfony/validator": "*",
		"symfony/web-link": "*",
		"symfony/webpack-encore-bundle": "*",
		"symfony/yaml": "*",
		"twig/cssinliner-extra": "*",
		"twig/inky-extra": "*",
		"twig/markdown-extra": "*",
		"vich/uploader-bundle": "*"
	},
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true

}
