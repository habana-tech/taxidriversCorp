{
    "name": "habanatech/taxidrivers-corp",
    "description": "An Habana.Tech project",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "dompdf/dompdf": "*",
        "easycorp/easyadmin-bundle": "~3.1",
        "erusev/parsedown": "^1.7",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "google/recaptcha": "~1.2.4",
        "liip/imagine-bundle": "~2.3.0",
        "roave/security-advisories": "dev-latest",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/asset": "^5.3",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/flex": "^1.9",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-client": "^5.3",
        "symfony/intl": "^5.3",
        "symfony/mailer": "^5.3",
        "symfony/messenger": "^5.3",
        "symfony/monolog-bundle": "^3.5",
        "symfony/notifier": "^5.3",
        "symfony/orm-pack": "^2.0",
        "symfony/process": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/serializer-pack": "~1.0.3",
        "symfony/string": "^5.3",
        "symfony/telegram-notifier": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^5.3",
        "symfony/web-link": "^5.3",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.3",
        "twig/cssinliner-extra": "~3.0",
        "twig/inky-extra": "^3.1",
        "twig/markdown-extra": "^3.3",
        "vich/uploader-bundle": "^1.15"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.24",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3"
        }
    }
}
